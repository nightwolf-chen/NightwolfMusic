<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>UpLoadUserHeadImage</servlet-name>
        <servlet-class>userUploader.UpLoadUserHeadImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoomImage</servlet-name>
        <servlet-class>userUploader.ZoomImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>makeCertPic</servlet-name>
        <servlet-class>Servlet.makeCertPic</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpLoadUserHeadImage</servlet-name>
        <url-pattern>/UpLoadUserHeadImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoomImage</servlet-name>
        <url-pattern>/ZoomImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>makeCertPic</servlet-name>
        <url-pattern>/makeCertPic</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
